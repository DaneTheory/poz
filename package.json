{
  "name": "poa",
  "version": "0.0.1",
  "description": "project omnipresent amenities",
  "repository": {
    "url": "ULIVZ/propa",
    "type": "git"
  },
  "main": "dist/propa.js",
  "module": "dist/propa.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test:cov": "BABEL_ENV=test jest src/** --coverage",
    "test": "BABEL_ENV=test npm run lint && jest",
    "lint": "xo",
    "prepublish": "npm run build",
    "build": "roly --config build/roly.config.js",
    "example": "poi --config build/poi.config.js",
    "build:example": "poi build --config build/poi.config.js",
    "gh": "gh-pages -d example/dist",
    "deploy": "npm run build:example && npm run gh"
  },
  "author": "ULIVZ <chl814@foxmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "packages"
    ]
  },
  "dependencies": {
    "archy": "^1.0.0",
    "chalk": "^2.3.0",
    "fs-extra": "^4.0.2",
    "git-config-path": "^1.0.1",
    "handlebars2": "^0.1.1",
    "inquirer": "^3.3.0",
    "map-stream": "0.0.7",
    "minimatch": "^3.0.4",
    "mitt": "^1.1.2",
    "parse-git-config": "^1.1.1",
    "recursive-readdir": "^2.2.1",
    "vinyl-fs": "^2.4.4"
  },
  "devDependencies": {
    "@types/inquirer": "0.0.35",
    "@types/node": "^8.0.47",
    "@types/vinyl": "^2.0.1",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-preset-env": "^1.4.0",
    "cross-spawn": "^5.1.0",
    "eslint-config-rem": "^3.0.0",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "handlebars2": "^0.1.0",
    "jest-cli": "^21.2.1",
    "poi": "^9.1.4",
    "raw-loader": "^0.5.1",
    "rollup-plugin-string": "^2.0.2",
    "roly": "^1.1.1",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem",
    "envs": [
      "jest"
    ]
  }
}
