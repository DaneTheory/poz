  "author": "ULIVZ <chl814@foxmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "git-config-path": "^1.0.1",
    "handlebars2": "^0.1.1",
    "mitt": "^1.1.2",
    "parse-git-config": "^1.1.1"
  },
  "devDependencies": {
    "babel-plugin-inline-import": "^2.0.6",
    "babel-preset-env": "^1.4.0",
    "eslint-config-rem": "^3.0.0",
    "gh-pages": "^1.0.0",
    "handlebars2": "^0.1.0",
    "jest-cli": "^21.2.1",
    "poi": "^9.1.4",
    "raw-loader": "^0.5.1",
    "rollup-plugin-string": "^2.0.2",
    "roly": "^1.1.1",
    "xo": "^0.18.0"
