{
  "name": "poa",
  "version": "0.0.1",
  "description": "project omnipresent amenities",
  "repository": {
    "url": "ULIVZ/propa",
    "type": "git"
  },
  "main": "dist/propa.js",
  "module": "dist/propa.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test:cov": "BABEL_ENV=test jest --coverage",
    "test": "BABEL_ENV=test npm run lint && jest",
