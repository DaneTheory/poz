{
  "name": "poa",
  "version": "0.0.0",
  "description": "project omnipresent amenities",
  "repository": {
    "url": "ulivz/poa",
    "type": "git"
  },
  "main": "dist/index.common.js",
  "bin": "cli.js",
  "files": [
    "dist", 
    "cli.js"
  ],
  "scripts": {
    "test": "jest --coveragenpm run lint && jest",
    "lint": "xo",
    "lint:fix": "npm run lint -- -fix",
    "prepublish": "npm run build",
    "build": "roly",
    "example": "poi",
    "build:example": "poi build",
    "gh": "gh-pages -d example/dist",
    "deploy": "npm run build:example && npm run gh"
  },
  "author": "ulivz <chl814@foxmail.com>",
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "/"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=4"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": { 
    "cac": "^4.0.0"
  },
  "devDependencies": { 
    "jest-cli": "^19.0.0",
    "eslint-config-rem": "^3.0.0",
    "xo": "^0.18.0",  
    "babel-preset-env": "^1.4.0",
    "gh-pages": "^1.0.0",
    "poi": "^9.1.4",
    "roly": "^1.1.1"
  }, 
  "xo": {
    "extends": "rem",
    "envs": [
      "jest"
    ]  
  } 
}